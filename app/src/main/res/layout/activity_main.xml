<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.bawei.myasd.MainActivity">

    <Button
        android:id="@+id/get_Synchronization"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get同步请求"/>

    <Button
        android:id="@+id/get_Asynchronous"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get异步请求"/>

    <Button
        android:id="@+id/post_Synchronization"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="post同步请求"/>

    <Button
        android:id="@+id/post_Asynchronous"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="post异步请求"/>

</LinearLayout>
